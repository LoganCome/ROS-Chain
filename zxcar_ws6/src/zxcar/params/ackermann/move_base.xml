<launch>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
	<rosparam file="$(find zxcar)/params/ackermann/costmap_common_params.yaml" command="load" ns="global_costmap" />
	<rosparam file="$(find zxcar)/params/ackermann/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find zxcar)/params/ackermann/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find zxcar)/params/ackermann/global_costmap_params.yaml" command="load" />
	<rosparam file="$(find zxcar)/params/ackermann/teb_local_planner_params.yaml" command="load" />
    <rosparam file="$(find zxcar)/params/ackermann/move_base_params.yaml" command="load" />
  </node>
</launch>
